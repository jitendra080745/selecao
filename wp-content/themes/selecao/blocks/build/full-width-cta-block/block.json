{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "custom-blocks/full-width-cat-block",
  "title": "Full Width Cat Block",
  "category": "custom-blocks",
  "icon": "align-full-width",
  "description": "A custom block with title, heading, description, link button, and a repeater field.",
  "supports": {
    "align": [
      "wide",
      "full"
    ]
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "buttonText": {
      "type": "string",
      "default": "Learn More"
    },
    "buttonUrl": {
      "type": "string",
      "default": "#"
    }
  },
  "textdomain": "custom-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./editor.scss",
  "style": "file:./style.scss"
}