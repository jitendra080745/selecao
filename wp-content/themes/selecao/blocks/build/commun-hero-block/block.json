{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "custom-blocks/commun-hero-block",
  "title": "commun Hero Block",
  "category": "custom-blocks",
  "icon": "networking",
  "description": "A custom block with title, heading, description, link button, and a repeater field.",
  "supports": {
    "align": [
      "wide",
      "full"
    ]
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "breadcrumbs": {
      "type": "array",
      "default": [
        {
          "title": "Home",
          "url": "/"
        }
      ]
    }
  },
  "textdomain": "custom-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./editor.scss",
  "style": "file:./style.scss"
}