{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "custom-blocks/hero-home-block",
  "title": "Hero Home Block",
  "category": "custom-blocks",
  "icon": "images-alt2",
  "description": "A  Block with a heading, description",
  "supports": {
    "align": [
      "full",
      "wide"
    ]
  },
  "attributes": {
    "repeater": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "default": ""
          },
          "description": {
            "type": "string",
            "default": ""
          },
          "buttonText": {
            "type": "string",
            "default": "Click Here"
          },
          "buttonUrl": {
            "type": "string",
            "default": "#"
          }
        }
      }
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./editor.css",
  "style": "file:./style.css"
}